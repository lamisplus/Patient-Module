trol\"\r\n                            name=\"sex\"\r\n                            id=\"sex\"\r\n                            {...register(\"sex\")}\r\n                            style={{ border: \"1px solid #014d88\" }}\r\n                          >\r\n                            <option value={\"\"}>Select Sex</option>\r\n                            {sexRows}\r\n                          </select>\r\n                          {errors.sex && <p>Select Sex</p>}\r\n                        </FormGroup>\r\n                      </div>\r\n                      <div className=\"form-group mb-3 col-md-2\">\r\n                        <FormGroup>\r\n                          <Label>Date Of Birth</Label>\r\n                          <div className=\"radio\">\r\n                            <label>\r\n                              <input\r\n                                type=\"radio\"\r\n                                value=\"Actual\"\r\n                                name=\"dateOfBirth\"\r\n                                defaultChecked\r\n                                {...register(\"dateOfBirth\")}\r\n                                onChange={(e) => handleDateOfBirthChange(e)}\r\n                                style={{ border: \"1px solid #014d88\" }}\r\n                              />{\" \"}\r\n                              Actual\r\n                            </label>\r\n                          </div>\r\n                          <div className=\"radio\">\r\n                            <label>\r\n                              <input\r\n                                type=\"radio\"\r\n                                value=\"Estimated\"\r\n                                name=\"dateOfBirth\"\r\n                                {...register(\"dateOfBirth\")}\r\n                                onChange={(e) => handleDateOfBirthChange(e)}\r\n                                style={{ border: \"1px solid #014d88\" }}\r\n                              />{\" \"}\r\n       